# Uncomment this line to define a global platform for your project
platform :ios, '9.0'
use_frameworks!

target 'MaidMe' do

# Lib for making request
pod 'Alamofire', '~> 3.0'
pod 'SwiftyJSON', '~> 2.3.0'

# Check internet connection
pod 'ReachabilitySwift', '~> 2.3'

# Validate Phone number
pod 'PhoneNumberKit', '~> 0.6'

# Encrypt data
pod 'CryptoSwift', '~> 0.2.3'

pod 'SSKeychain'

# Custom cell with action
pod 'SWTableViewCell', '~> 0.3.7'


# Google Places search address
pod 'GooglePlaces'
pod 'GooglePlacePicker'
pod 'GoogleMaps'
pod 'GooglePlacesSearchController'

# Image caching
pod 'SDWebImage'

# Activity Indicator
pod 'SVProgressHUD'

# Realm
pod 'RealmSwift', '~> 2.3.0'

# SDVersion
pod 'SDVersion'

# Tooltip view
pod 'EasyTipView', '0.2.2'

# Fabric
pod 'Fabric', '~> 1.6'
pod 'Crashlytics'

end

# Payfort StartSDK
pod 'StartSDK', :git => 'https://github.com/payfort/start-ios-sdk.git'

target 'MaidMeTests' do

end

target 'MaidMeUITests' do

end

